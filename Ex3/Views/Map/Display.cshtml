
@{
    ViewBag.Title = "display";
}


<html>
<body background="~/Content/export-map-share.png" style="background-size:100% 100%; background-attachment:fixed">
    <canvas id="myCanvas" style="background-size:100% 100%"></canvas>
    <script>
            var lon = ('@ViewBag.lon' + 180) * (canvas.height / 180);
            var lat = ('@ViewBag.lat' + 90) * (canvas.height / 360);
            var c = document.getElementById("myCanvas");
            var context1 = c.getContext("2d");
            context1.beginPath();
            context1.lineWidth = 1;
        context1.arc(lon, lat, 6, 0, 2 * Math.PI);
            context1.fillStyle = 'red';
            context1.fill();
            context1.stroke();
            context1.closePath();

        
    </script>

</body>
</html>